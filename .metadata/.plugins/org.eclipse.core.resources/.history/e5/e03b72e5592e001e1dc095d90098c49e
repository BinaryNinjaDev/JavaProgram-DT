// a. Code reuseability
//b. Dynamic Binding or runtime polymorphism or method overriding
class Base1 {
	int  x;
	int  y;
	
	void show() {
		System.out.println(x);
		System.out.println(y);
	}

	class child1 extends Base1{
		void get(int x, int y) {
			this.x=x;
			this.y=y;
		}
	}

	public static void main(String[] args) {
	   child1 c1 = new child1();
	   c1 .get(10,20);
	   c1.show();
	  
	}

}
