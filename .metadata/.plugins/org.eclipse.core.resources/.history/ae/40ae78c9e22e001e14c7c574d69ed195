
 class Base1 {
	 int x;
	 int y;
	 
	 void show() {
		 System.out.println(x);
		 System.out.println(y);
	 }
 }
class Child1 extends Base1{
	void get(int x, int y)
	{
		this.x=x;
		this.y=y;
	}


	public static void main(String[] args) {
		// TODO Auto-generated method stub
        Child1 c1 = new Child1();
        c1.get(10, 20);
        c1.show();
	}
}


